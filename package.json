    "@types/facebook-pixel": "0.0.19",
    "@types/fs-extra": "8.0.0",
    "@types/google.analytics": "0.0.40",
    "@types/jasmine": "3.3.16",
    "@types/node": "~12.6.9",
    "@types/segment-analytics": "0.0.32",
